{
  "name": "signaling-server",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "start": "node ./dist/app.js",
    "build": "tsc --build && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.6.3"
  }
}
